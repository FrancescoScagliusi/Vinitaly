###  http://www.semanticweb.org/pisca/vinitaly#DiCorpo
:DiCorpo rdf:type owl:Class ;
         owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                      owl:onProperty :determina ;
                                                      owl:someValuesFrom :AbbinamentoSecondiPiatti
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :gradoAlcolico ;
                                                      owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                           owl:onDatatype xsd:float ;
                                                                           owl:withRestrictions ( [ xsd:minInclusive "13.5"^^xsd:float
                                                                                                  ]
                                                                                                  [ xsd:maxInclusive "14.0"^^xsd:float
                                                                                                  ]
                                                                                                )
                                                                         ]
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
         rdfs:subClassOf :Struttura .


###  http://www.semanticweb.org/pisca/vinitaly#DiGrandeStruttura
:DiGrandeStruttura rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                owl:onProperty :determina ;
                                                                owl:someValuesFrom :AbbinamentoSecondiPiatti
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :gradoAlcolico ;
                                                                owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                     owl:onDatatype xsd:float ;
                                                                                     owl:withRestrictions ( [ xsd:minExclusive "14.0"^^xsd:float
                                                                                                            ]
                                                                                                          )
                                                                                   ]
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                   rdfs:subClassOf :Struttura .


###  http://www.semanticweb.org/pisca/vinitaly#DiMedioCorpo
:DiMedioCorpo rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Class ;
                                                           owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                           owl:onProperty :determina ;
                                                                           owl:someValuesFrom :AbbinamentoPrimiPiatti
                                                                         ]
                                                                         [ rdf:type owl:Restriction ;
                                                                           owl:onProperty :determina ;
                                                                           owl:someValuesFrom :AbbinamentoSecondiPiatti
                                                                         ]
                                                                       )
                                                         ]
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :gradoAlcolico ;
                                                           owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                owl:onDatatype xsd:float ;
                                                                                owl:withRestrictions ( [ xsd:minInclusive "12.5"^^xsd:float
                                                                                                       ]
                                                                                                       [ xsd:maxInclusive "13.0"^^xsd:float
                                                                                                       ]
                                                                                                     )
                                                                              ]
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:subClassOf :Struttura .

