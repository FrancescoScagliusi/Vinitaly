@prefix : <http://www.semanticweb.org/pisca/vinitaly#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/pisca/vinitaly> .

<http://www.semanticweb.org/pisca/vinitaly> rdf:type owl:Ontology ;
                                             owl:imports <http://www.ontologydesignpatterns.org/cp/owl/set.owl> .

#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#short
xsd:short owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                owl:onDatatype xsd:double ;
                                owl:withRestrictions ( [ xsd:minInclusive "13.5"^^xsd:double
                                                       ]
                                                       [ xsd:maxInclusive "14.0"^^xsd:double
                                                       ]
                                                     )
                              ] .


#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> rdfs:domain :Cantina ;
                                                                              rdfs:range :Vino .


###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> rdfs:domain :Vino ;
                                                                               rdfs:range :Cantina .


###  http://www.semanticweb.org/pisca/vinitaly#aromatizza
:aromatizza rdf:type owl:ObjectProperty ;
            owl:inverseOf :haAroma .


###  http://www.semanticweb.org/pisca/vinitaly#haAroma
:haAroma rdf:type owl:ObjectProperty ;
         rdfs:domain :Vino ;
         rdfs:range :Aroma .


###  http://www.semanticweb.org/pisca/vinitaly#haDolcezza
:haDolcezza rdf:type owl:ObjectProperty ;
            rdfs:domain :Vino ;
            rdfs:range :Dolcezza .


###  http://www.semanticweb.org/pisca/vinitaly#haOrigineIn
:haOrigineIn rdf:type owl:ObjectProperty ;
             owl:inverseOf :origina ;
             rdf:type owl:TransitiveProperty ;
             rdfs:domain :Vitigno ;
             rdfs:range :Origine .


###  http://www.semanticweb.org/pisca/vinitaly#haStruttura
:haStruttura rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Vino .


###  http://www.semanticweb.org/pisca/vinitaly#momentoPortata
:momentoPortata rdf:type owl:ObjectProperty ;
                rdfs:domain :Portata .


###  http://www.semanticweb.org/pisca/vinitaly#origina
:origina rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/pisca/vinitaly#portataAbbinataCon
:portataAbbinataCon rdf:type owl:ObjectProperty ;
                    owl:inverseOf :vinoAbbinatoCon ;
                    owl:propertyChainAxiom ( :tipoPortata
                                             :éConsigliato
                                           ) .


###  http://www.semanticweb.org/pisca/vinitaly#possedutaDa
:possedutaDa rdf:type owl:ObjectProperty ;
             owl:inverseOf :éproprietarioDi ;
             rdfs:domain :Cantina ;
             rdfs:range :Produttore .


###  http://www.semanticweb.org/pisca/vinitaly#prodottoDa
:prodottoDa rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/pisca/vinitaly#produce
:produce rdf:type owl:ObjectProperty ;
         rdfs:domain :Vitigno ;
         rdfs:range :Vino .


###  http://www.semanticweb.org/pisca/vinitaly#realizzatoDa
:realizzatoDa rdf:type owl:ObjectProperty ;
              owl:propertyChainAxiom ( <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf>
                                       :possedutaDa
                                     ) .


###  http://www.semanticweb.org/pisca/vinitaly#tipoPortata
:tipoPortata rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Portata ;
             rdfs:range :TipoPortata .


###  http://www.semanticweb.org/pisca/vinitaly#vinoAbbinatoA
:vinoAbbinatoA rdf:type owl:ObjectProperty ;
               owl:inverseOf :éConsigliato .


###  http://www.semanticweb.org/pisca/vinitaly#vinoAbbinatoCon
:vinoAbbinatoCon rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/pisca/vinitaly#vinoOriginatoIn
:vinoOriginatoIn rdf:type owl:ObjectProperty ;
                 owl:propertyChainAxiom ( :prodottoDa
                                          :haOrigineIn
                                        ) .


###  http://www.semanticweb.org/pisca/vinitaly#éConsigliato
:éConsigliato rdf:type owl:ObjectProperty ,
                       owl:TransitiveProperty ;
              rdfs:domain :Portata ;
              rdfs:range :Vino .


###  http://www.semanticweb.org/pisca/vinitaly#éproprietarioDi
:éproprietarioDi rdf:type owl:ObjectProperty ;
                 rdfs:domain :Produttore ;
                 rdfs:range :Cantina .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/pisca/vinitaly#annata
:annata rdf:type owl:DatatypeProperty ;
        rdfs:domain :Vino ;
        rdfs:range xsd:dateTime .


###  http://www.semanticweb.org/pisca/vinitaly#coloreBacca
:coloreBacca rdf:type owl:DatatypeProperty ;
             rdfs:domain :Vitigno ;
             rdfs:range xsd:string .


###  http://www.semanticweb.org/pisca/vinitaly#denominazione
:denominazione rdf:type owl:DatatypeProperty ;
               rdfs:domain :Vino ;
               rdfs:range xsd:string .


###  http://www.semanticweb.org/pisca/vinitaly#grado
:grado rdf:type owl:DatatypeProperty ;
       rdfs:range xsd:float .


###  http://www.semanticweb.org/pisca/vinitaly#gradoAlcolico
:gradoAlcolico rdf:type owl:DatatypeProperty ;
               rdfs:domain :Vino ;
               rdfs:range xsd:float .


###  http://www.semanticweb.org/pisca/vinitaly#haTassoAlcolico
:haTassoAlcolico rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Vino ;
                 rdfs:range xsd:double .


###  http://www.semanticweb.org/pisca/vinitaly#pressione
:pressione rdf:type owl:DatatypeProperty ;
           rdfs:domain :Vino ;
           rdfs:range xsd:float .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/pisca/vinitaly#Aroma
:Aroma rdf:type owl:Class .


###  http://www.semanticweb.org/pisca/vinitaly#AromaPrimario
:AromaPrimario rdf:type owl:Class ;
               rdfs:subClassOf :Aroma .


###  http://www.semanticweb.org/pisca/vinitaly#AromaPrimarioErba
:AromaPrimarioErba rdf:type owl:Class ;
                   rdfs:subClassOf :AromaPrimario .


###  http://www.semanticweb.org/pisca/vinitaly#AromaPrimarioFiore
:AromaPrimarioFiore rdf:type owl:Class ;
                    rdfs:subClassOf :AromaPrimario .


###  http://www.semanticweb.org/pisca/vinitaly#AromaPrimarioFrutta
:AromaPrimarioFrutta rdf:type owl:Class ;
                     rdfs:subClassOf :AromaPrimario .


###  http://www.semanticweb.org/pisca/vinitaly#AromaSecondario
:AromaSecondario rdf:type owl:Class ;
                 rdfs:subClassOf :Aroma .


###  http://www.semanticweb.org/pisca/vinitaly#AromaSecondarioLievito
:AromaSecondarioLievito rdf:type owl:Class ;
                        rdfs:subClassOf :AromaSecondario .


###  http://www.semanticweb.org/pisca/vinitaly#AromaSecondarioMalolattica
:AromaSecondarioMalolattica rdf:type owl:Class ;
                            rdfs:subClassOf :AromaSecondario .


###  http://www.semanticweb.org/pisca/vinitaly#AromaTerziario
:AromaTerziario rdf:type owl:Class ;
                rdfs:subClassOf :Aroma .


###  http://www.semanticweb.org/pisca/vinitaly#AromaTerziarioAltroInvecchiamento
:AromaTerziarioAltroInvecchiamento rdf:type owl:Class ;
                                   rdfs:subClassOf :AromaTerziario .


###  http://www.semanticweb.org/pisca/vinitaly#AromaTerziarioMaturazioneInLegno
:AromaTerziarioMaturazioneInLegno rdf:type owl:Class ;
