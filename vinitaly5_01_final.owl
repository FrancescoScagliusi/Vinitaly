@prefix : <http://www.semanticweb.org/pisca/vinitaly#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/pisca/vinitaly> .

<http://www.semanticweb.org/pisca/vinitaly> rdf:type owl:Ontology ;
                                             owl:imports <http://www.ontologydesignpatterns.org/cp/owl/set.owl> ;
                                             <http://purl.org/dc/elements/1.1/creator> "Piccolo Nicola" ,
                                                                                       "Scagliusi Francesco" ;
                                             <http://purl.org/dc/elements/1.1/date> "2022-01-25" ;
                                             <http://purl.org/dc/elements/1.1/description> "L'ontoglia descrive il dominio dei vini, in particolare andando a descrivere la varie tipologie (per colore, per struttura, per aroma), l'origine del vino, la cantina e il produttore. Inoltre viene descritta una portata, specificandone il tipo, in modo tale da poter modellare l'abbinamento di un vino."@it ;
                                             <http://purl.org/dc/elements/1.1/title> "Vinitaly"@it ;
                                             <http://purl.org/vocab/vann/preferredNamespacePrefix> "http://www.semanticweb.org/pisca/vinitaly" ,
                                                                                                   "vinitaly" ;
                                             rdfs:label "Vinitaly"@it .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
<http://purl.org/dc/elements/1.1/date> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
<http://purl.org/vocab/vann/preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
<http://purl.org/vocab/vann/preferredNamespaceUri> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#short
xsd:short owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                owl:onDatatype xsd:double ;
                                owl:withRestrictions ( [ xsd:minInclusive "13.5"^^xsd:double
                                                       ]
                                                       [ xsd:maxInclusive "14.0"^^xsd:double
                                                       ]
                                                     )
                              ] .


#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> rdfs:domain :Cantina ;
                                                                              rdfs:range :Vino .


###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> rdfs:domain :Vino ;
                                                                               rdfs:range :Cantina .


###  http://www.semanticweb.org/pisca/vinitaly#aromatizza
:aromatizza rdf:type owl:ObjectProperty ;
            owl:inverseOf :haAroma .


###  http://www.semanticweb.org/pisca/vinitaly#haAroma
:haAroma rdf:type owl:ObjectProperty ;
         rdfs:domain :Vino ;
         rdfs:range :Aroma .


###  http://www.semanticweb.org/pisca/vinitaly#haDosaggio
:haDosaggio rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :Vino ;
            rdfs:range :Dosaggio ;
            rdfs:comment "Collega il vino ad un tipo di dosaggio (secco, brut, dry, ...)"@it ;
            rdfs:label "ha dosaggio"@it .


###  http://www.semanticweb.org/pisca/vinitaly#haOrigineIn
:haOrigineIn rdf:type owl:ObjectProperty ;
             owl:inverseOf :origina ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :Vitigno ;
             rdfs:range :Origine ;
             rdfs:comment "Collega il Vitigno alla sua regione italiana di origine"@it ;
             rdfs:label "haOrigineIn"@it .


###  http://www.semanticweb.org/pisca/vinitaly#haStruttura
:haStruttura rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Vino .


###  http://www.semanticweb.org/pisca/vinitaly#momentoPerDegustarlo
:momentoPerDegustarlo rdf:type owl:ObjectProperty ;
                      rdfs:domain :Vino ;
                      rdfs:range :Occasione ;
                      rdfs:comment "Collega un vino all'occasione in cui viene degustato"@it ;
                      rdfs:label "momento per degustarlo"@it .


###  http://www.semanticweb.org/pisca/vinitaly#origina
:origina rdf:type owl:ObjectProperty ;
         rdfs:comment "Inverso della proprietà \"origina\" collega quindi una Regione ai Vitigni che presenta"@it ;
         rdfs:label "origina"@it .


###  http://www.semanticweb.org/pisca/vinitaly#portataAbbinataCon
:portataAbbinataCon rdf:type owl:ObjectProperty ;
                    owl:inverseOf :vinoAbbinatoCon ;
                    owl:propertyChainAxiom ( :tipoPortata
                                             :éConsigliato
                                           ) ;
                    rdfs:comment "Property chain per collegare una portata al tipo di vino che è consigliato consumare durante la stessa"@it ;
                    rdfs:label "portata abbinata con"@it .


###  http://www.semanticweb.org/pisca/vinitaly#possedutaDa
:possedutaDa rdf:type owl:ObjectProperty ;
             owl:inverseOf :éproprietarioDi ;
             rdfs:domain :Cantina ;
             rdfs:range :Produttore ;
             rdfs:comment "Inversa di \" è proprietario di \", collega la cantina al produtture che ne è proprietario"@it ;
             rdfs:label "posseduta da"@it .


###  http://www.semanticweb.org/pisca/vinitaly#prodottoDa
:prodottoDa rdf:type owl:ObjectProperty ;
            owl:inverseOf :produce ;
            rdfs:comment "Inversa della proprietà \"produce\", collega quindi un vino ai vitigni utilizzati per produrlo"@it ;
            rdfs:label "prodotto da"@it .


###  http://www.semanticweb.org/pisca/vinitaly#produce
:produce rdf:type owl:ObjectProperty ;
         rdfs:domain :Vitigno ;
         rdfs:range :Vino ;
         rdfs:comment "Collega un vitigno ad un vino da cui è prodotto"@it ;
         rdfs:label "produce"@it .


###  http://www.semanticweb.org/pisca/vinitaly#realizzatoDa
:realizzatoDa rdf:type owl:ObjectProperty ;
              owl:propertyChainAxiom ( <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf>
                                       :possedutaDa
                                     ) ;
              rdfs:comment "Property chain per collegare un vino al suo produttore, passando per la cantina"@it ;
              rdfs:label "realizzato da"@it .


###  http://www.semanticweb.org/pisca/vinitaly#somiglia
:somiglia rdf:type owl:ObjectProperty ,
                   owl:TransitiveProperty ;
          rdfs:domain :Vino ;
          rdfs:range :Vino .


###  http://www.semanticweb.org/pisca/vinitaly#tipoPortata
:tipoPortata rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Portata ;
             rdfs:range :TipoPortata ;
             rdfs:comment "Collega una portata (es: Bistecca di manzo) ad un tipo portata (es: Antipasto di carne, di pesce, ecc...)"@it ;
             rdfs:label "tipo portata"@it .


###  http://www.semanticweb.org/pisca/vinitaly#vinoAbbinatoA
:vinoAbbinatoA rdf:type owl:ObjectProperty ;
               owl:inverseOf :éConsigliato ;
               rdfs:comment "Inversa di \"è consigliato\", collega un tipo di vino (es: vino bianco leggero) al tipo di portata a cui si abbina meglio (es: antipasto di pesce)"@it ;
               rdfs:label "vino abbinato a"@it .


###  http://www.semanticweb.org/pisca/vinitaly#vinoAbbinatoCon
:vinoAbbinatoCon rdf:type owl:ObjectProperty ;
                 rdfs:comment "Inversa della proprietà \"portata abbinata con\", collega una portata (es: Bistecca di manzo) ad un tipo di vino consigliato (es: VinoRossoLeggero)"@it ;
                 rdfs:label "vino abbinato con"@it .


###  http://www.semanticweb.org/pisca/vinitaly#vinoOriginatoIn
:vinoOriginatoIn rdf:type owl:ObjectProperty ;
                 owl:propertyChainAxiom ( :prodottoDa
                                          :haOrigineIn
                                        ) ;
                 rdfs:comment "Property chain che collega un vino alla sua regione di origine, passando per il vitigno"@it ;
                 rdfs:label "vino originato in"@it .


###  http://www.semanticweb.org/pisca/vinitaly#éConsigliato
:éConsigliato rdf:type owl:ObjectProperty ;
              rdfs:domain :TipoPortata ;
              rdfs:range :Vino ;
              rdfs:comment "Collega un tipo portata (es: Antipasto Carne, Primo di pesce) ad un tipo di vino consigliato (es: Vino bianco leggero)"@it ;
              rdfs:label "é consigliato"@it .


###  http://www.semanticweb.org/pisca/vinitaly#éproprietarioDi
:éproprietarioDi rdf:type owl:ObjectProperty ;
                 rdfs:domain :Produttore ;
                 rdfs:range :Cantina ;
                 rdfs:comment "Collega il produttore (es: San Marzano) alla cantina di cui è proprietario"@it ;
                 rdfs:label "è proprietario di"@it .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/pisca/vinitaly#annata
:annata rdf:type owl:DatatypeProperty ;
        rdfs:domain :Vino ;
        rdfs:range xsd:string .


###  http://www.semanticweb.org/pisca/vinitaly#coloreBacca
:coloreBacca rdf:type owl:DatatypeProperty ;
             rdfs:domain :Vitigno ;
             rdfs:range xsd:string .


###  http://www.semanticweb.org/pisca/vinitaly#denominazione
:denominazione rdf:type owl:DatatypeProperty ;
               rdfs:domain :Vino ;
               rdfs:range xsd:string .


###  http://www.semanticweb.org/pisca/vinitaly#gradoAlcolico
:gradoAlcolico rdf:type owl:DatatypeProperty ;
               rdfs:domain :Vino ;
               rdfs:range xsd:float .


###  http://www.semanticweb.org/pisca/vinitaly#pressione
:pressione rdf:type owl:DatatypeProperty ;
           rdfs:domain :Vino ;
           rdfs:range xsd:float .


###  http://www.semanticweb.org/pisca/vinitaly#prezzo
:prezzo rdf:type owl:DatatypeProperty ;
        rdfs:domain :Vino ;
        rdfs:range xsd:float .


#################################################################
#    Classes
#################################################################

###  http://data.lirmm.fr/ontologies/food#Dish
<http://data.lirmm.fr/ontologies/food#Dish> rdf:type owl:Class ;
                                            owl:equivalentClass :Portata .


###  http://rdfs.co/bevon/FruitWine
<http://rdfs.co/bevon/FruitWine> rdf:type owl:Class ;
                                 owl:equivalentClass :VinoAromatizzato .


###  http://rdfs.co/bevon/RedWine
<http://rdfs.co/bevon/RedWine> rdf:type owl:Class ;
                               owl:equivalentClass :VinoRosso .


###  http://rdfs.co/bevon/WhiteWine
<http://rdfs.co/bevon/WhiteWine> rdf:type owl:Class ;
                                 owl:equivalentClass :VinoBianco .


###  http://rdfs.co/bevon/Wine
<http://rdfs.co/bevon/Wine> rdf:type owl:Class ;
                            owl:equivalentClass :Vino .


###  http://www.semanticweb.org/pisca/vinitaly#Aroma
:Aroma rdf:type owl:Class ;
       owl:equivalentClass <https://dbpedia.org/ontology/taste> ;
       rdfs:comment "Classe per rappresentare gli aromi che possono essere associati al vino"@it ;
       rdfs:label "Aroma"@it .


###  http://www.semanticweb.org/pisca/vinitaly#AromaPrimario
:AromaPrimario rdf:type owl:Class ;
               rdfs:subClassOf :Aroma ;
               rdfs:comment "Classe per rappresentare gli aromi primari del vino, contenuti nelle foglie, nella polpa e nella buccia degli acini."@it ;
               rdfs:label "Aroma primario"@it .


###  http://www.semanticweb.org/pisca/vinitaly#AromaPrimarioErba
:AromaPrimarioErba rdf:type owl:Class ;
                   rdfs:subClassOf :AromaPrimario ;
                   rdfs:comment "Classe degli aromi erbacei"@it ;
                   rdfs:label "Aroma primario erba"@it .


###  http://www.semanticweb.org/pisca/vinitaly#AromaPrimarioFiore
:AromaPrimarioFiore rdf:type owl:Class ;
                    rdfs:subClassOf :AromaPrimario ;
                    rdfs:comment "Classe degli aromi floreali"@it ;
                    rdfs:label "Aroma primario fiore"@it .


###  http://www.semanticweb.org/pisca/vinitaly#AromaPrimarioFrutta
:AromaPrimarioFrutta rdf:type owl:Class ;
                     rdfs:subClassOf :AromaPrimario ;
                     rdfs:comment "Classe degli aromi fruttati"@it ;
                     rdfs:label "Aroma primario frutta"@it .


###  http://www.semanticweb.org/pisca/vinitaly#AromaSecondario
:AromaSecondario rdf:type owl:Class ;
                 rdfs:subClassOf :Aroma ;
                 rdfs:comment "Classe per rappresentare gli aromi secondari di un vino, generati durante la fermentazione"@it ;
                 rdfs:label "Aroma secondario"@it .


###  http://www.semanticweb.org/pisca/vinitaly#AromaSecondarioLievito
:AromaSecondarioLievito rdf:type owl:Class ;
                        rdfs:subClassOf :AromaSecondario ;
                        rdfs:comment "Classe per rappresentare aromì ad opera di lieviti durante la fermentazione"@it ;
                        rdfs:label "Aroma secondario lievito"@it .


###  http://www.semanticweb.org/pisca/vinitaly#AromaSecondarioMalolattica
:AromaSecondarioMalolattica rdf:type owl:Class ;
                            rdfs:subClassOf :AromaSecondario ;
                            rdfs:comment "Classe di aromi dovuti ad un tipo di fermentazione malolattica"@it ;
                            rdfs:label "Aroma secondario malolattica"@it .


###  http://www.semanticweb.org/pisca/vinitaly#AromaTerziario
:AromaTerziario rdf:type owl:Class ;
                rdfs:subClassOf :Aroma ;
                rdfs:comment "Classe per rappresentare gli aromi terziari di un vino, generati durante l'invecchiamento"@it .


###  http://www.semanticweb.org/pisca/vinitaly#AromaTerziarioAltroInvecchiamento
:AromaTerziarioAltroInvecchiamento rdf:type owl:Class ;
                                   rdfs:subClassOf :AromaTerziario ;
                                   rdfs:comment "Classe di aromi derivanti da maturazione con altri tipi di invecchiamento"@it ;
                                   rdfs:label "Aroma terziario altro invecchiamento"@it .


###  http://www.semanticweb.org/pisca/vinitaly#AromaTerziarioMaturazioneInLegno
:AromaTerziarioMaturazioneInLegno rdf:type owl:Class ;
                                  rdfs:subClassOf :AromaTerziario ;
                                  rdfs:comment "Classe di aromi derivanti dalla maturazione in legno"@it ;
                                  rdfs:label "Aroma terziario maturazione in legno"@it .


###  http://www.semanticweb.org/pisca/vinitaly#AromaTerziarioOssidazione
:AromaTerziarioOssidazione rdf:type owl:Class ;
                           rdfs:subClassOf :AromaTerziario ;
                           rdfs:comment "Classe di aromi derivanti da reazione di ossidazione"@it ;
                           rdfs:label "Aroma terziario ossidazione"@it .


###  http://www.semanticweb.org/pisca/vinitaly#Cantina
:Cantina rdf:type owl:Class ;
         owl:equivalentClass <https://dbpedia.org/ontology/Winery> ;
         rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/set.owl#Set> ;
         rdfs:comment "Classe per rappresentare una particolare cantina di un dato produttore, che conterrà un insieme di vini"@it ;
         rdfs:label "Cantina"@it .


###  http://www.semanticweb.org/pisca/vinitaly#ColoreVino
:ColoreVino rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :prodottoDa ;
                                  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass :Vitigno
                                ] ;
            rdfs:subClassOf :Vino ;
            rdfs:comment "Classe per rappresentare un vino di un particolare colore, definito dal colore della bacca del vitigno da cui è originato"@it ;
            rdfs:label "vino colore"@it .


###  http://www.semanticweb.org/pisca/vinitaly#Dosaggio
:Dosaggio rdf:type owl:Class ;
          rdfs:comment "Classe per definire la dolcezza di particolare vino, ottenuta dagli zuccheri residui presenti nel vino, responsabili della sua maggiore o minore dolcezza"@it ;
          rdfs:label "dosaggio"@it .


###  http://www.semanticweb.org/pisca/vinitaly#DosaggioVinoBollicine
:DosaggioVinoBollicine rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Class ;
                                             owl:oneOf ( :brut
                                                         :brut-nature
                                                         :demi-sec
                                                         :doux
                                                         :dry
                                                         :extra-brut
                                                         :extra-dry
                                                       )
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty [ owl:inverseOf :haDosaggio
                                                            ] ;
                                             owl:someValuesFrom :VinoSpumante
                                           ] ;
                       rdfs:subClassOf :Dosaggio ;
                       rdfs:comment "Classe enumerata che definisce una scala di dolcezza dei vini spumanti"@it ;
                       rdfs:label "dosaggio vino bollicine"@it .


###  http://www.semanticweb.org/pisca/vinitaly#DosaggioVinoFermo
:DosaggioVinoFermo rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Class ;
                                         owl:oneOf ( :abboccato
                                                     :amabile
                                                     :dolce
                                                     :secco
                                                   )
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty [ owl:inverseOf :haDosaggio
                                                        ] ;
                                         owl:someValuesFrom [ rdf:type owl:Class ;
                                                              owl:unionOf ( :VinoFermo
                                                                            :VinoFrizzante
                                                                          )
                                                            ]
                                       ] ;
                   rdfs:subClassOf :Dosaggio ;
                   rdfs:comment "Classe enumerata che definisce una scala di dolcezza dei vini fermi e frizzanti"@it ;
                   rdfs:label "dosaggio vino fermo"@it .


###  http://www.semanticweb.org/pisca/vinitaly#Occasione
:Occasione rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:oneOf ( :aperitivo
                                             :bicchiereQuotidiano
                                             :cenaFormale
                                             :cenaTraAmici
                                             :cocktailParty
                                             :occasioneFormale
                                             :occasioneInformale
                                             :pranzoInFamiglia
                                           )
                               ] ;
           rdfs:comment "Classe per rappresentare il momento in cui degustare un vino"@it ;
           rdfs:label "Occasione"@it .


###  http://www.semanticweb.org/pisca/vinitaly#Origine
:Origine rdf:type owl:Class ;
         owl:equivalentClass <https://dbpedia.org/page/Regions_of_Italy> ,
                             [ rdf:type owl:Class ;
                               owl:oneOf ( :Abruzzo
                                           :Basilicata
                                           :Calabria
                                           :Campania
                                           :Emilia-Romagna
                                           :Friuli-Venezia-Giulia
                                           :Lazio
                                           :Liguria
                                           :Lombardia
                                           :Marche
                                           :Molise
                                           :Piemonte
                                           :Puglia
                                           :Sardegna
                                           :Sicilia
                                           :Toscana
                                           :Trentino-Alto-Adige
                                           :Umbria
                                           :ValleDAosta
                                           :Veneto
                                         )
                             ] ;
         rdfs:comment "Classe enumerata per la definizione delle regioni italiane"@it ;
         rdfs:label "regione di origine"@it .


###  http://www.semanticweb.org/pisca/vinitaly#Portata
:Portata rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :tipoPortata ;
                               owl:someValuesFrom :TipoPortata
                             ] ;
         rdfs:comment "Descrive una Portata (es: Bistecca di manzo) definita da un Tipo Portata (es: Secondo di carne)"@it ;
         rdfs:label "Portata"@it .


###  http://www.semanticweb.org/pisca/vinitaly#Produttore
:Produttore rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Class ;
                                  owl:unionOf ( [ rdf:type owl:Restriction ;
                                                  owl:onProperty :éproprietarioDi ;
                                                  owl:someValuesFrom :Cantina
                                                ]
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty [ owl:inverseOf :realizzatoDa
                                                                 ] ;
                                                  owl:someValuesFrom :Vino
                                                ]
                                              )
                                ] ;
            rdfs:comment "Classe per rappresentare un particolare produttore di vino, proprietario di una data cantina"@it ;
            rdfs:label "produttore di vino"@it .


###  http://www.semanticweb.org/pisca/vinitaly#StrutturaVino
:StrutturaVino rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :gradoAlcolico ;
                                     owl:someValuesFrom xsd:float
                                   ] ;
               rdfs:subClassOf :Vino ;
               rdfs:comment "Classe per rappresentare un vino con una particolare struttura, definita sulla base del grado alcolico del vino"@it ;
               rdfs:label "struttura vino"@it .


###  http://www.semanticweb.org/pisca/vinitaly#TipoAntipasto
:TipoAntipasto rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Class ;
                                     owl:oneOf ( :TipoAntipastoCarne
                                                 :TipoAntipastoPesce
                                               )
                                   ] ;
               rdfs:subClassOf :TipoPortata ;
               rdfs:comment "Classe enumerata contenente i diversi tipi di antipasto (carne e pesce)"@it ;
               rdfs:label "tipo di antipasto"@it .


###  http://www.semanticweb.org/pisca/vinitaly#TipoPortata
:TipoPortata rdf:type owl:Class ;
             rdfs:comment "Definisce il tipo di una portata"@it ;
             rdfs:label "tipo portata"@it .


###  http://www.semanticweb.org/pisca/vinitaly#TipoPrimoPiatto
:TipoPrimoPiatto rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Class ;
                                       owl:oneOf ( :TipoPrimoPiattoCarne
                                                   :TipoPrimoPiattoPesce
                                                 )
                                     ] ;
                 rdfs:subClassOf :TipoPortata ;
                 rdfs:comment "Classe enumerata contenente i diversi tipi di primi piatti (carne o pesce)"@it ;
                 rdfs:label "tipo primo piatto"@it .


###  http://www.semanticweb.org/pisca/vinitaly#TipoSecondoPiatto
:TipoSecondoPiatto rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Class ;
                                         owl:oneOf ( :TipoSecondoPiattoCarne
                                                     :TipoSecondoPiattoPesce
                                                   )
                                       ] ;
                   rdfs:subClassOf :TipoPortata ;
                   rdfs:comment "Classe enumerata contenente i diversi tipi di secondi piatti (carne e pesce)"@it ;
                   rdfs:label "tipo di secondo piatto"@it .


###  http://www.semanticweb.org/pisca/vinitaly#TipologiaVino
:TipologiaVino rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :pressione ;
                                     owl:someValuesFrom xsd:float
                                   ] ;
